{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18",
    "codebase": "default",
    "deploy": {
      "api": {
        "gen": 2,
        "region": "us-central1"
      }
    }
  },
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.z*", "**/node_modules/**"]
  },
  "emulators": {
    "hosting": {
      "host": "localhost",
      "port": 5005
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  }
}
